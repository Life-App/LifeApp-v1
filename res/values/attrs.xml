<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Circle">
        <attr name="circleBackground" format="integer" />
        <attr name="firstChildPosition">
            <enum name="East" value="0" />
            <enum name="South" value="90" />
            <enum name="West" value="180" />
            <enum name="North" value="270" />
        </attr>
        <attr name="rotateToCenter" format="boolean" />
        <attr name="isRotating" format="boolean" />
        <attr name="speed" format="integer" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="name" format="string" />
    </declare-styleable>
    
        <!-- note: to re-use an existing Android attribute not already used by the superclass, name should have prefix "android:" and do not define a format -->
    <declare-styleable name="NumberPickerDialogPreference">
        <attr name="android:max" />
        <attr name="min" format="integer" />
    </declare-styleable>
    
</resources>